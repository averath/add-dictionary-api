{
    "name": "add-dictionary-api",
    "version": "0.3.0",
    "main": "index.js",
    "private": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "engines": {
        "node": ">=20.11.0",
        "yarn": ">=1.22.0",
        "npm": "please-use-yarn"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc -w",
        "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/app.js",
        "start2": "ts-node -r tsconfig-paths/register ./src/app.ts",
        "dev": "npx nodemon --exec ts-node -r tsconfig-paths/register ./src/app.ts",
        "test": "jest --forceExit --detectOpenHandles  --watchAll --maxWorkers=1",
        "lint": "npx eslint src",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier src --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix"
    },
    "repository": "https://github.com/averath/add-dictionary-api.git",
    "license": "unlicensed",
    "dependencies": {
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^3.6.0",
        "dotenv": "^16.4.5",
        "eslint": "^9.1.1",
        "express": "^4.19.2",
        "express-ip": "^1.0.4",
        "express-rate-limit": "^7.2.0",
        "express-useragent": "^1.0.15",
        "fs-extra": "^11.2.0",
        "helmet": "^7.1.0",
        "jest": "^29.7.0",
        "joi": "^17.13.0",
        "knex": "^3.1.0",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "mysql2": "^3.9.7",
        "nodemon": "^3.1.0",
        "reflect-metadata": "^0.2.2",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.7",
        "@types/cors": "^2.8.17",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.21",
        "@types/express-useragent": "^1.0.5",
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.12",
        "@types/lodash": "^4.17.0",
        "@types/morgan": "^1.9.9",
        "@types/reflect-metadata": "^0.1.0",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.7.1",
        "@typescript-eslint/parser": "^7.7.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "prettier": "^3.2.5",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.4.5"
    }
}
